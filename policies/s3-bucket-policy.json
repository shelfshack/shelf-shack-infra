{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowPublicReadItem",
      "Effect": "Allow",
      "Principal": "*",
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::shelfshack-dev-uploads/item_images/*"
    },
    {
      "Sid": "AllowPublicReadProfile",
      "Effect": "Allow",
      "Principal": "*",
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::shelfshack-dev-uploads/profile_photos/*"
    },
    {
      "Sid": "AllowS3Uploads",
      "Effect": "Allow",
      "Principal": "*",
      "Action": [
        "s3:PutObject",
        "s3:PutObjectAcl"
      ],
      "Resource": "arn:aws:s3:::shelfshack-dev-uploads/*"
    }
  ]
}

